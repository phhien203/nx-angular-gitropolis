    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/workspace-library/**/*.ts",
          "libs/workspace-library/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
